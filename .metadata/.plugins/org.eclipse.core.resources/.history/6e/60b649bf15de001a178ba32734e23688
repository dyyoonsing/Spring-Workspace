package hw.shape02;

import org.springframework.beans.factory.annotation.Value;
import org.springframework.stereotype.Component;

@Component
public class Rectangle extends Shape {
	
	@Value("5")
	private int width;
	@Value("12")
	private int height;
	
	@Override
	public String getShape() {
		
		return "사각형";
	}

	public int getWidth() {
		return width;
	}

	public int getHeight() {
		return height;
	}
	
}
