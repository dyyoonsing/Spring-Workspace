package hw.shape02;

public abstract class Shape {
	
	protected int width;
	protected int height;
	protected int radius;
	
	public abstract void getArea();

	public Shape(int width, int height) {
		super();
		this.width = width;
		this.height = height;
	}

	public Shape(int radius) {
		super();
		this.radius = radius;
	}
	
	
}
